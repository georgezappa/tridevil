{
  "name": "tridevil",
  "version": "0.0.5",
  "description": "My first attempt to create a WEBGL project using three.js",
  "main": "main.js",
  "author": "georgezappa",
  "scripts": {
    "start": "serve ",
    "oldbuild": "babel src -d public --copy-files && rollup -c rollout.config.js",
    "build": "babel src -d public --copy-files && workbox injectManifest ./workbox-config.js && rollup -c rollout.config.js",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel build start watch",
    "deploy": "",
    "push": "",
    "lint": "eslint src --ext js",
    "test": "yarn run build && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgezappa/tridevil.git"
  },
  "keywords": [
    "node",
    "three.js",
    "WEBGL",
    "javascript",
    "babel",
    "jest",
    "rollup"
  ],
  "bugs": {
    "url": "https://github.com/georgezappa/tridevil/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/georgezappa/tridevil#readme",
  "dependencies": {
    "@babel/preset-env": "^7.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-jest": "^24.8.0",
    "install": "^0.13.0",
    "jest": "^24.8.0",
    "npm": "^6.10.1",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "serve": "^11.0.2"
  }
}
